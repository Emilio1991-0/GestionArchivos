import java.io.File;
import java.io.IOException;

public class CrearArchivo {

	public static void Crear(String[] args) {
	    try {
	      File myObj = new File("Ejercicio1.txt");
	      if (myObj.createNewFile()) {
	        System.out.println("Archivo creado: " + myObj.getName());
	      } else {
	        System.out.println("El archivo ya exixte");
	      }
	    } catch (IOException e) {
	      System.out.println("Ha ocurrido un error");
	      e.printStackTrace();
	    }
	  }

}
